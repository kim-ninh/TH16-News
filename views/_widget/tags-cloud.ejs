<div class="<%- display %> d-none">
    <div class="h3 bg-primary text-white pl-1">TAGS CLOUD</div>
    <div class="bg-light px-1" style="font-size: 0.9rem;">
        <% var count = 0; %>
        <% for (const tag in site.tags.data) { %>
            <% if (count < site.tags.length && count < 20) { %>
                <% var tagName = site.tags.data[tag].name %>
                    <a href="#" class="d-inline-block mr-1 text-dark">
                        <%- tagName %>
                    </a>
            <% } %>
            <% count++; %>
        <% } %>
    </div>
</div>